            <version>3.9.1</version>
        </dependency>
        <dependency>
            <groupId>org.jsoup</groupId>
            <artifactId>jsoup</artifactId>
            <version>1.9.2</version>
        </dependency>
        <dependency>
            <groupId>yuqiao</groupId>
            <artifactId>search</artifactId>
            <version>0.0.1-SNAPSHOT</version>
        </dependency>
        <dependency>
